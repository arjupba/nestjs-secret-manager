#!/bin/sh

npx --no -- commitlint --edit $1 || (
        echo '游땫游游녦游땫 Commit Message Format is not correct! 游땫游游녦游땫
                Make the required changes listed above, add changes and try to commit again.
                '
        false;
)

echo '
       九九九九 Verfied Commit Message Format 九九九九

       '